#include"dog.h"
#include<stdlib.h>
/**
 * int_dog - init new dog
 * @d: potner to struct dog;
 * @name:name
 * @age:age
 * @owner:ow
 */
void init_dog(struct dog *d, char *name, float age, char *owner)
{
	if (d == NULL)
		d = malloc(sizeof(struct dog));
	d->name = name;
	d->age = age;
	d->owner = owner;
}
